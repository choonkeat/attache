# A sample Gemfile
source "https://rubygems.org"

ruby '2.2.3'

gem 'rack'
gem 'paperclip'
gem 'puma'
gem 'fog'
gem 'excon', '~> 0.44.4' # https://github.com/excon/excon/issues/482

gem 'sys-filesystem'
gem 'disk_store'

gem 'foreman'
gem 'sidekiq'
gem 'sucker_punch' # single-process Ruby asynchronous processing library

group :development, :test do
  gem 'rspec'
  gem 'shoulda', require: false
  gem 'guard-rspec', require: false
end
